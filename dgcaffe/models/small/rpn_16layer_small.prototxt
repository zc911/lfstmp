    num_output: 256
    kernel_size: 1
    pad: 0
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu7_cls"
  type: "ReLU"
  bottom: "conv7_cls"
  top: "conv7_cls"
}

layer {
  name: "conv7_reg"
  type: "Convolution"
  bottom: "conv6"
  top: "conv7_reg"
  param {
    lr_mult: 1
