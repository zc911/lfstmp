  name: "conv5_up"
  type: "Deconvolution"
  bottom: "conv5_3" 
  top: "conv5_up"
  convolution_param {
    kernel_size: 4 
    stride: 2
    num_output: 128
    group: 128
    pad: 1
    weight_filler{ 
      #type: "bilinear" 
      type: "xavier" 
    } 
    bias_term: false
  }
  param { 
    lr_mult: 0 
    decay_mult: 0 
  }
}

#concat layer-----------------------------------------------------------
layer {
  name: "concat5"
  type: "Concat"
  bottom: "conv5_up"
  bottom: "conv4_3"
  top: "concat5"
  concat_param {
    concat_dim: 1
  }
}

#Layer 6-------------------------------------------------
layer {
  name: "conv6"
  type: "Convolution"
  bottom: "concat5"
  top: "conv6"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu6"
  type: "ReLU"
  bottom: "conv6"
  top: "conv6"
}

